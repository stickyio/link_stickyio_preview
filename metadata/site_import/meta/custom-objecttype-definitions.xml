<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <custom-type type-id="stickyioCampaigns">
        <display-name xml:lang="x-default">sticky.io Campaigns</display-name>
        <description xml:lang="x-default">Object containing JSON representation of all current sticky.io Campaigns</description>
        <staging-mode>source-to-target</staging-mode>
        <storage-scope>site</storage-scope>
        <key-definition attribute-id="campaigns">
            <display-name xml:lang="x-default">Campaigns</display-name>
            <description xml:lang="x-default">sticky.io Campaign JSON object</description>
            <type>string</type>
            <min-length>0</min-length>
        </key-definition>
        <attribute-definitions>
            <attribute-definition attribute-id="jsonData">
                <display-name xml:lang="x-default">JSON Data</display-name>
                <description xml:lang="x-default">The JSON campaign data from sticky.io</description>
                <type>text</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
        </attribute-definitions>
        <group-definitions>
            <attribute-group group-id="stickyio">
                <display-name xml:lang="x-default">sticky.io</display-name>
                <attribute attribute-id="campaigns"/>
                <attribute attribute-id="creationDate" system="true"/>
                <attribute attribute-id="lastModified" system="true"/>
                <attribute attribute-id="jsonData"/>
            </attribute-group>
        </group-definitions>
    </custom-type>
</metadata>
